{
  "root": true,
  "extends": ["@react-native-community", "prettier"],
  "plugins": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "semi": 0,
    "@typescript-eslint/no-unused-vars": ["warn"],
    "no-unused-vars": "off",
    "no-trailing-spaces": "off",
    "jsx-quotes": "off",
    "react-native/no-inline-styles": "off",
    "spaced-comment": "warn",
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
